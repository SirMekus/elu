{
    "name": "sirmekus/elu",
    "description": "Simple and easy file uploader for Laravel-based apps",
    "keywords": ["laravel", "image uploader"],
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.82|^9.0"
    },
    "require-dev": {
        "intervention/image": "^2.7"
    },
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Elu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Fortify\\EluServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "SirMekus",
            "email": "mekus600@gmail.com"
        }
    ]
}
